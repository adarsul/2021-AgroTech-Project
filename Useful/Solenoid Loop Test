int solenoidPin = 25; //output on board for Solenoid valve
void setup() {
  pinMode(solenoidPin, OUTPUT); //set solenoid output
}

void loop() {
  digitalWrite(solenoidPin, HIGH); //open solenoid and wait 1 sec
  delay(1000);
  digitalWrite(solenoidPin, LOW); //close solenoid and wait 1 sec
  delay(1000);
}
